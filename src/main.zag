import std;
import c;

import raylib;

fn main() void {
    raylib.InitWindow(1280, 720, "Hello world!");

    while (!raylib.WindowShouldClose()) {
        raylib.BeginDrawing();
        raylib.ClearBackground(raylib.Color { r: 255, g: 255, b: 255, a: 255 });
        raylib.EndDrawing();
    }

    raylib.CloseWindow();
}
