fn main() i32 {
    let a = 2; // const int a = 2;
    let b = &a; // const int * const b = &a;
    let mut d = &a; // const int * d = &a;
    let e = 4; // const int e = 4;
    d = &e; // d = &e

    let mut a2 = 2; // int a2 = 2;
    let b2 = &a2; // const int * const b2 = &a2;
    let c2 = &mut a2; // int * const c2 = &a2;
    let mut d2 = &mut a2; // int * d2 = &a2;
}
