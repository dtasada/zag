struct MyStruct {
	x: i32,
	y: i32,

    fn setX(self: &mut MyStruct, new_x: i32) void {
        self.x = new_x;
    }

	fn add(lhs: MyStruct, rhs: MyStruct) MyStruct {
		return MyStruct {
			x: lhs.x + rhs.x,
			y: lhs.y + rhs.y,
		};
	}
}

fn main() void {
    let my_obj = MyStruct {
        x: 1,
        y: 1,
    };

    let array: [2 * 2]u8 = my_obj;

    let cond: i32 = if (0) 0 else if (1) 1 else 2;

    let setX = MyStruct.setX;
    setX(&my_obj, 4);
}
