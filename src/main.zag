struct MyStruct {
	x: i32,
	y: i32,

    fn setX(self: &mut MyStruct, new_x: i32) void {
        self.x = new_x;
    }

	fn add(lhs: MyStruct, rhs: MyStruct) MyStruct {
		return MyStruct {
			x: lhs.x + rhs.x,
			y: lhs.y + rhs.y,
		};
	}
}

fn add(lhs: i32, rhs: i32) i32 {
    return lhs + rhs;
}

fn main() i32 {
    let mut my_obj = MyStruct { x: 1, y: 1 };
    let obj_ref = &my_obj;

    my_obj.setX(2);
    my_obj.add(obj_ref);
}
