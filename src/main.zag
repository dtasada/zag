    fn set_x(self: &mut MyStruct, new_x: i32) void {
        self.x = new_x;
    }

    fn add(lhs: MyStruct, rhs: MyStruct) MyStruct {
        return MyStruct {
            x: lhs.x + rhs.x,
            y: lhs.y + rhs.y,
        };
    }
}

