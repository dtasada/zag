import std;
import c;

// Test generic parameter in signature
fn identity<T>(val: T) T {
    return val;
}

fn main() i32 {
    let x = identity<i32>(42);
    c.printf("identity: %d\n", x);

    let y = identity<f32>(42);
    c.printf("identity: %f\n", y);
    
    let ms = std.alloc<i32>();
    let b = std.Box<i32> { content: 123 };
    b.print();

    let gs = std.Box<i32> {a: 2};
}
