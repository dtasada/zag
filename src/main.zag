import lib;

struct MyStruct {
    x, y: i32,
    z: f32
}

union MyUnion {
    f: f32,
    i: i32,
    c: u8,

    fn print(self: MyUnion) void {
        lib.printf("self: %c", self.c);
    }
}

fn main() i32 {
    let a = 1;
    let b = 2;
    let c = 3;
    let d = 4;

    let mu = MyUnion { i: 65 };

    mu.print();
}
