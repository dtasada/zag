pub bind fn printf(fmt: &c_char, args...) c_int;

pub bind fn malloc(size: usize) &mut void;

pub bind fn free(ptr: &mut void) void;

pub bind fn realloc(ptr: &mut void, len: usize) &mut void;

pub bind fn pow(a: f64, b: f64) f64;

pub bind fn memcpy(dst: &mut void, src: &void, len: usize) &mut void;

pub bind fn memmove(dst: &mut void, src: &void, len: usize) &mut void;
